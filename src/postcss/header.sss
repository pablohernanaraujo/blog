header
	width: 100%
	height: 74px
	background-color: var(--color-blanco)
	color: var(--color-texto)
	position: fixed
	top: 0

.btn-header
	text-decoration: none
	font-size: 13px
	margin-left: 20px
	line-height: 74px
	color: var(--color-texto)

.buscador
	float: right
	margin-right: 20px
	padding: 5px 10px
	margin-top: 22px
	border: none
	border-bottom: 1px solid rgba(0,0,0,0.1)
	font-size: 14px
	width: 200px

input[type="text"]:focus
	outline: none
